{
    "manifest_version": 3,
    "name": "Let Me Leave, Bro",
    "description": "Do you hate \"Are you SURE you want to leave this page?\" Javascript popups? So do I. This is an extension that prevents those.",
    "version": "2024.12.30.0851",
    "author": {
        "email": "software@rosettast0ned.com"
    },
    "homepage_url": "https://github.com/rosettast0ned/let-me-leave-bro",
    "incognito": "split",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "icons": {
        "16": "let_me_leave_bro16.png",
        "32": "let_me_leave_bro32.png",
        "48": "let_me_leave_bro48.png",
        "128": "let_me_leave_bro128.png"
    },
    "action": {
        "default_icon": {
            "16": "let_me_leave_bro16.png",
            "32": "let_me_leave_bro32.png",
            "48": "let_me_leave_bro48.png",
            "128": "let_me_leave_bro128.png"
        }
    }
}
